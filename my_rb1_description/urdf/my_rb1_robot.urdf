<?xml version="1.0"?>
<robot name="rb1_robot">    
    <!-- *************************************************** -->
    <!-- GAZEBO TAGS -->
    <!-- Gazebo tags - Links material-->
    <gazebo reference="base_link">
        <material>Gazebo/Black</material>
    </gazebo>
    <gazebo reference="right_wheel">
        <material>Gazebo/Grey</material>
        <mu1>1</mu1>
        <mu2>1</mu2>
    </gazebo>
    <gazebo reference="left_wheel">
        <material>Gazebo/Grey</material>
        <mu1>1</mu1>
        <mu2>1</mu2>
    </gazebo>
    <gazebo reference="back_caster">
        <material>Gazebo/Orange</material>
        <mu1>0</mu1>
        <mu2>0</mu2>
    </gazebo>
    <gazebo reference="front_caster">
        <material>Gazebo/Orange</material>
        <mu1>0</mu1>
        <mu2>0</mu2>
    </gazebo>
    <gazebo reference="joint_base_link_front_laser">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <gazebo reference="front_laser">
        <material>Gazebo/DarkGrey</material>
    </gazebo>

    <!-- Gazebo tags - preserveFixedJoint-->
    <gazebo reference="joint_base_link_back_caster">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <gazebo reference="joint_base_link_front_caster">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <gazebo reference="joint_base_link_front_laser">
        <preserveFixedJoint>true</preserveFixedJoint>
    </gazebo>
    <!-- *************************************************** -->

    <!-- *************************************************** -->
    <!-- ROBOT MODEL -->
    <!-- Link - base_link -->
    <link name="base_link">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.3" radius="0.25"/>
            </geometry>
            <material name="yellow">
                <color rgba="0.949 0.933 0.067 0.5"/>
            </material>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder length="0.3" radius="0.25"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="25"/>
            <!-- Inertia tensor of a solid cylinder of radius r, height h and mass m. 
                 https://en.wikipedia.org/wiki/List_of_moments_of_inertia#List_of_3D_inertia_tensors -->
            <inertia ixx="0.5781" ixy="0.0" ixz="0.0" iyy="0.5781" iyz="0.0" izz="0.78125"/>
        </inertial>
    </link>

    <!-- Joint - base_footprint/base_link -->
    <joint name="joint_base_footprint_base_link" type="fixed">
        <origin xyz="0 0 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="base_footprint"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>
    <!-- Link - base_footprint -->
    <link name="base_footprint"/>

    <!-- Joint -  base_link/right_wheel -->
    <joint name="joint_base_link_right_wheel" type="continuous">
        <origin xyz="0.0 -0.2 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="right_wheel"/>
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>    
    <!-- Link - right_wheel -->
    <link name="right_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.5708 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.05"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="1.5708 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.05"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1"/>
            <!-- Inertia tensor of a solid cylinder of radius r, height h and mass m. 
                 https://en.wikipedia.org/wiki/List_of_moments_of_inertia#List_of_3D_inertia_tensors -->
            <inertia ixx="0.0008" ixy="0.0" ixz="0.0" iyy="0.0008" iyz="0.0" izz="0.0012"/>
        </inertial>
    </link>

    <!-- Joint -  base_link/left_wheel -->
    <joint name="joint_base_link_left_wheel" type="continuous">
        <origin xyz="0.0 0.2 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="left_wheel"/>
        <axis rpy="0 0 0" xyz="0 1 0"/>
        <limit effort="10000" velocity="1000"/>
        <joint_properties damping="1.0" friction="1.0"/>
    </joint>    
    <!-- Link - left_wheel -->
    <link name="left_wheel">
        <visual>
            <origin xyz="0 0 0" rpy="1.5708 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.05"/>
            </geometry>
            <material name="black">
                <color rgba="0 0 0 1"/>
            </material>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="1.5708 0 0"/>
            <geometry>
                <cylinder length="0.05" radius="0.05"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="1"/>
            <!-- Inertia tensor of a solid cylinder of radius r, height h and mass m. 
                 https://en.wikipedia.org/wiki/List_of_moments_of_inertia#List_of_3D_inertia_tensors -->
            <inertia ixx="0.0008" ixy="0.0" ixz="0.0" iyy="0.0008" iyz="0.0" izz="0.0012"/>
        </inertial>
    </link>
    
    <!-- Joint -  base_link/back_caster -->
    <joint name="joint_base_link_back_caster" type="fixed">
        <origin xyz="-0.1 0.0 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="back_caster"/>
        <joint_properties damping="1.0" friction="0"/>
    </joint>    
    <!-- Link - back_caster -->
    <link name="back_caster">
        <!-- Back caster wheel support -->
        <visual>
            <origin xyz="0 0 0.01" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.05" />
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0.01" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.05" />
            </geometry>
        </collision>
        <!-- Back caster wheel -->
        <visual>
            <origin xyz="0 0 -0.025" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="purple">
                <color rgba="0.427 0 0.8 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 -0.025" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <!-- Inertia tensor of a solid sphere of radius r and mass m. 
                 https://en.wikipedia.org/wiki/List_of_moments_of_inertia#List_of_3D_inertia_tensors -->
            <inertia ixx="0.0005" ixy="0.0" ixz="0.0" iyy="0.0005" iyz="0.0" izz="0.0005"/>
        </inertial>
    </link>

    <!-- Joint -  base_link/front_caster -->
    <joint name="joint_base_link_front_caster" type="fixed">
        <origin xyz="0.1 0.0 -0.15" rpy="0 0 0"/>
        <parent link="base_link"/>
        <child link="front_caster"/>
        <joint_properties damping="1.0" friction="0"/>
    </joint>    
    <!-- Link - front_caster -->
    <link name="front_caster">
        <!-- Front caster wheel support -->
        <visual>
            <origin xyz="0 0 0.01" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.05" />
            </geometry>
            <material name="Black">
                <color rgba="0 0 0 1" />
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 0.01" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.05" />
            </geometry>
        </collision>
        <!-- Front caster wheel -->
        <visual>
            <origin xyz="0 0 -0.025" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
            <material name="purple">
                <color rgba="0.427 0 0.8 1"/>
            </material>
        </visual>
        <collision>
            <origin xyz="0 0 -0.025" rpy="0 0 0"/>
            <geometry>
                <sphere radius="0.025"/>
            </geometry>
        </collision>

        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <mass value="0.5"/>
            <!-- Inertia tensor of a solid sphere of radius r and mass m. 
                 https://en.wikipedia.org/wiki/List_of_moments_of_inertia#List_of_3D_inertia_tensors -->
            <inertia ixx="0.0005" ixy="0.0" ixz="0.0" iyy="0.0005" iyz="0.0" izz="0.0005"/>
        </inertial>
    </link>

    <!-- Joint -  base_link/front_laser -->
    <joint name="joint_base_link_front_laser" type="fixed">
        <origin xyz="0.25 0.0 0.075" rpy="3.1416 0 0"/>
        <parent link="base_link"/>
        <child link="front_laser"/>
        <joint_properties damping="1.0" friction="0"/>
    </joint>    
    <!-- Link - front_laser -->
    <link name="front_laser">
        <visual>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
            <material name="Blue">
                <color rgba="0 0.035 1 1" />
            </material>
        </visual>

        <collision>
            <origin xyz="0 0 0" rpy="0 0 0"/>
            <geometry>
                <cylinder radius="0.05" length="0.04"/>
            </geometry>
        </collision>
        
        <inertial>
            <origin xyz="0 0 0" rpy="0 0 0" />
            <mass value="0.5" />
            <inertia ixx="0.000252666666667" ixy="0" ixz="0" iyy="0.000252666666667" iyz="0" izz="0.0005"/>
        </inertial>
    </link>
    <!-- *************************************************** -->
</robot>